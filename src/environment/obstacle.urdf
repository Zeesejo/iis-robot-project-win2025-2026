<?xml version="1.0" encoding="utf-8"?>
<!-- Obstacle cube: 0.4m side, mass=10kg, color set programmatically -->
<robot name="obstacle">
  <link name="obstacle_link">
    <inertial>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="0.13" ixy="0" ixz="0" iyy="0.13" iyz="0" izz="0.13"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <geometry><box size="0.4 0.4 0.4"/></geometry>
      <material name="blue"><color rgba="0.0 0.0 1.0 1"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <geometry><box size="0.4 0.4 0.4"/></geometry>
    </collision>
  </link>
</robot>
