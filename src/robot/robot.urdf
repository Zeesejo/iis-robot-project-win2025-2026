<?xml version="1.0"?>
<robot name="iis_robot">

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="grey">
    <color rgba="0.5 0.5 0.5 1"/>
  </material>
  <material name="black">
    <color rgba="0.1 0.1 0.1 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.5 0.4 0.1"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.5 0.4 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="12.0"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.05"/>
    </inertial>
  </link>

  <link name="fl_wheel">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.002"/>
    </inertial>
  </link>

  <joint name="fl_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="fl_wheel"/>
    <origin xyz="0.15 0.225 0" rpy="1.5708 0 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <link name="fr_wheel">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.002"/>
    </inertial>
  </link>

  <joint name="fr_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="fr_wheel"/>
    <origin xyz="0.15 -0.225 0" rpy="1.5708 0 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <link name="bl_wheel">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.002"/>
    </inertial>
  </link>

  <joint name="bl_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="bl_wheel"/>
    <origin xyz="-0.15 0.225 0" rpy="1.5708 0 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <link name="br_wheel">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.002"/>
    </inertial>
  </link>

  <joint name="br_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="br_wheel"/>
    <origin xyz="-0.15 -0.225 0" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- <link name="rgbd_camera_link"/>
  <joint name="camera_joint" type="fixed">
    <parent link="gripper_base"/>
    <child link="rgbd_camera_link"/>
    <origin xyz="0.0 0.0 0.06" rpy="0 0.2 0"/>
  </joint> -->

  <link name="torso_link">
    <visual>
      <geometry>
          <box size="0.3 0.3 0.5"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
          <box size="0.3 0.3 0.5"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.02" iyy="0.02" izz="0.02"/>
    </inertial>
  </link>

  <joint name="torso_fixed_joint" type="fixed">
    <parent link="base_link"/>
    <child link="torso_link"/>
    <origin xyz="0 0 0.3"/>
  </joint>

  <link name="rgbd_camera_link"/>
  <joint name="camera_joint" type="fixed">
    <parent link="torso_link"/>
    <child link="rgbd_camera_link"/>
   <origin xyz="0.25 0 0.45" rpy="0 0 0"/>
  </joint>

  <link name="lidar_link"/>
  <joint name="lidar_joint" type="fixed">
    <parent link="torso_link"/>
    <child link="lidar_link"/>
    <origin xyz="0 0 0.18" rpy="0 0 0"/>
  </joint>

  <link name="lift_carriage">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.01" iyy="0.01" izz="0.01"/>
    </inertial>
  </link>

  <joint name="lift_joint" type="fixed">
    <parent link="torso_link"/>
    <child link="lift_carriage"/>
    <origin xyz="0 0 0.22" rpy="0 0 0"/>
  </joint>

  <link name="arm_base">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.01" iyy="0.01" izz="0.01"/>
    </inertial>
  </link>

  <joint name="arm_base_joint" type="revolute">
    <parent link="lift_carriage"/>
    <child link="arm_base"/>
    <origin xyz="0 0 0.05"/>
    <axis xyz="0 0 1"/>
    <limit effort="10.0" lower="-3.14" upper="3.14" velocity="1.0"/>
  </joint>

  <link name="shoulder_link">
    <visual>
      <origin xyz="0 0 0.2"/>
      <geometry>
        <box size="0.05 0.05 0.4"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.2"/>
      <geometry>
        <box size="0.05 0.05 0.4"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.2"/>
      <mass value="1.0"/>
      <inertia ixx="0.02" iyy="0.02" izz="0.01"/>
    </inertial>
  </link>

  <joint name="shoulder_joint" type="revolute">
    <parent link="arm_base"/>
    <child link="shoulder_link"/>
    <origin xyz="0 0 0.025"/>
   <axis xyz="1 0 0"/>
    <limit effort="10.0" lower="-1.0" upper="1.57" velocity="1.0"/>
  </joint>

  <link name="elbow_link">
    <visual>
      <origin xyz="0 0 0.15"/>
      <geometry>
        <box size="0.04 0.04 0.27"/>
        
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.15"/>
      <geometry>
        <box size="0.04 0.04 0.3"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.15"/>
      <mass value="0.8"/>
      <inertia ixx="0.01" iyy="0.01" izz="0.01"/>
    </inertial>
  </link>
  <joint name="elbow_joint" type="revolute">
    <parent link="shoulder_link"/>
    <child link="elbow_link"/>
    <origin xyz="0 0 0.4"/>
    <axis xyz="1 0 0"/>
    <limit effort="10.0" lower="-0.5" upper="2.0" velocity="1.0"/>
  </joint>
  <link name="wrist_link_start">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.02"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.8"/>
      <inertia ixx="0.01" iyy="0.01" izz="0.01"/>
    </inertial>
  </link>
  <link name="wrist_pitch_link">
    <visual>
      <geometry>
        <box size="0.04 0.05 0.04"/>
      </geometry>
      <material name="black"/>
        <origin xyz="0 0 0.02"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.04 0.05 0.04"/>
      </geometry>
        <origin xyz="0 0 0.02"/>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001"/>
    </inertial>
  </link>
    <!-- Wrist pitch: elbow -> wrist_pitch_link -->
  <joint name="wrist_pitch_joint" type="revolute">
    <parent link="elbow_link"/>
    <child link="wrist_pitch_link"/>
    <origin xyz="0 0 0.29"/>
    <axis xyz="1 0 0"/>
    <limit effort="5.0" lower="-1.57" upper="1.57" velocity="1.0"/>
  </joint>

  <!-- Fixed link at the end of wrist pitch -->
  <joint name="wrist_joint_start" type="fixed">
    <parent link="wrist_pitch_link"/>
    <child link="wrist_link_start"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- Wrist roll now attached to wrist_link_start (not directly to wrist_pitch_link) -->
  <joint name="wrist_roll_joint" type="revolute">
    <parent link="wrist_link_start"/>
    <child link="wrist_roll_link"/>
    <origin xyz="0 0 0.056"/>
    <axis xyz="0 0 1"/>
    <limit effort="5.0" lower="-3.14" upper="3.14" velocity="1.0"/>
  </joint>


  <link name="wrist_roll_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.03"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.03"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001"/>
    </inertial>
  </link>

    <link name="wrist_roll_link_cup">
    <visual>
      <geometry>
        <cylinder length="0.02" radius="0.033"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.02" radius="0.033"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001"/>
    </inertial>
  </link>
  <joint name="wrist_roll_joint_cup" type="fixed">
    <parent link="wrist_roll_link"/>
    <child link="wrist_roll_link_cup"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>
  <link name="gripper_base">
    <visual>
      <geometry>
        <box size="0.05 0.1 0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.1 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001"/>
    </inertial>
  </link>

  <joint name="gripper_base_joint" type="fixed">
    <parent link="wrist_roll_link"/>
    <child link="gripper_base"/>
    <origin xyz="0 0 0.05"/>
  </joint>

  <link name="left_finger">
    <visual>
      <geometry>
        <box size="0.08 0.01 0.02"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.01 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="left_finger_joint" type="prismatic">
    <parent link="gripper_base"/>
    <child link="left_finger"/>
    <origin xyz="0.04 0.04 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="5.0" lower="-0.04" upper="0" velocity="0.5"/>
  </joint>

  <link name="right_finger">
    <visual>
      <geometry>
        <box size="0.08 0.01 0.02"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.01 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="right_finger_joint" type="prismatic">
    <parent link="gripper_base"/>
    <child link="right_finger"/>
    <origin xyz="0.04 -0.04 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="5.0" lower="0" upper="0.04" velocity="0.5"/>
  </joint>

  <link name="touch_sensor_link"/>

  <joint name="touch_sensor_joint" type="fixed">
    <parent link="gripper_base"/>
    <child link="touch_sensor_link"/>
    <origin xyz="0.05 0 0" rpy="0 0 0"/>
  </joint>

</robot>